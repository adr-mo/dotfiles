    "ValidatorArrayOf": {
        "prefix": "ValidatorArrayOf",
        "body": [
            "#[Assert\\NotNull]",
            "#[Assert\\Type('array')]",
            "#[Assert\\All(",
            "    new Assert\\Type('${1:integer}'),",
            ")]"
        ]
    },
    "bValue": {
